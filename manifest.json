{
  "manifest_version": 2,
  "name": "Fiat2LTC",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-latest.min.js", "content.js"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
        "frameGBP.html",
        "frameUSD.html",
        "frameEUR.html",
        "frameAUD.html",
        "frameBRL.html",
        "frameCAD.html",
        "frameCZK.html",
        "frameKRW.html",
        "frameMXN.html",
        "framePLN.html",
        "frameRON.html",
        "frameTRY.html"
  ],
  "browser_action": {
    "default_icon": "f2lLogo1-small.png",
    "default_popup": "options.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}